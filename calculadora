    private void jButton55ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // boton *
        igual=true;
        inicio=true;

        if(operacion1 = true){
            valor1 = Double.parseDouble(jTextField1.getText());
            jTextField2.setText("");
            jTextField2.setText(jTextField1.getText()+"*");
            operacion1 = false;
        }else{
            if(operacion2 = true){
                valor2 = Double.parseDouble(jTextField1.getText());
                jTextField2.setText(jTextField1.getText()+"*");
                operacion2 = false;
            }else{
                jTextField2.setText(jTextField1.getText()+"*");
                Operaciones(resultado,valor2);
            }
        }
        tipoOperaciones = "*";
    }                                         

    private void jButton56ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // boton porcentaje
        valor2 = Double.parseDouble(jTextField1.getText());
        jTextField2.setText(jTextField2.getText()+jTextField1.getText());
        jTextField1.setText((valor1*valor2)/100+" ");
        igual = true;
    }                                         

    private void jButton57ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        //boton+
        igual=true;
        inicio=true;

        if(operacion1 = true){
            valor1 = Double.parseDouble(jTextField1.getText());
            jTextField2.setText("");
            jTextField2.setText(jTextField1.getText()+"+");
