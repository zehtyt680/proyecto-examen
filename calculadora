    private void jButton59ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // boton /
        igual=true;
        inicio=true;

        if(operacion1 = true){
            valor1 = Double.parseDouble(jTextField1.getText());
            jTextField2.setText("");
            jTextField2.setText(jTextField1.getText()+"/");
            operacion1 = false;
        }else{
