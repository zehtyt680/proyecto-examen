private void Operaciones(double valor1, double valor2){
// switch para que evalue la variable tipoOperaciones
switch(tipoOperaciones){
   //clase con las funcionees de suma
    case "+":
        resultado = valor1 + valor2;
        jTextField1.setText(resultado+"");
        valor1 = Double.parseDouble(jTextField1.getText());
        break;
        //clase con las funcionees de resta
    case "-":
        resultado = valor1 - valor2;
        jTextField1.setText(resultado+"");
        valor1 = Double.parseDouble(jTextField1.getText());
        break;
        //clase con las funcionees de multiplicacion
    case "*":
        resultado = valor1 * valor2;
        jTextField1.setText(resultado+"");
        valor1 = Double.parseDouble(jTextField1.getText());
        break;
        //clase con las funcionees de divicion
    case "/":
        if(valor2 == 0){
            jTextField1.setText("Error");
            break;
        }else{
        resultado = valor1 / valor2;
        jTextField1.setText(resultado+"");
        valor1 = Double.parseDouble(jTextField1.getText());
        break;
        }
        // case de la funcion que saca la potencia de un numero
    case "potencia":
        resultado = Math.pow(valor1,valor2);
        jTextField1.setText(resultado+" ");
        break;
    case "raizx":
        if (valor2 <=0 ){
            jTextField1.setText("Error");
            break;  
        }else{
        jTextField2.setText(valor2 + "yroot");
        a = 1;
        c = a/valor2;
        jTextField1.setText(Math.pow(valor1,c) + " ");
        break;
        }
}
}
