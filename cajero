    private void jButton_cancelarActionPerformed(java.awt.event.ActionEvent evt) {                                                 
        // boton cancelar
        txt_cantidad.setText(" ");
    }                                                

    private void jButton_borrarActionPerformed(java.awt.event.ActionEvent evt) {                                               
        // boton borrar
        txt_cantidad.setText(txt_cantidad.getText().substring(0,txt_cantidad.getText().length() - 1));
        
        if (txt_cantidad.getText().equals(" ")) {
            txt_cantidad.setText(" ");
        }
    }                                              

    private void jButton_aceptarActionPerformed(java.awt.event.ActionEvent evt) {                                                
        // boton aceptar
        if (txt_cantidad.getText().equals(" ")) {
            opcionDepositar = false;
            opcionRetirar = false;
            
            panel_opcion.setVisible(false);
            jButton_depositar.setEnabled(true);
            jButton_retirar.setEnabled(true);
            txt_cantidad.setText(" ");
            
            jButton_1.setEnabled(false);
        jButton_2.setEnabled(false);
        jButton_3.setEnabled(false);
        jButton_4.setEnabled(false);
        jButton_5.setEnabled(false);
        jButton_6.setEnabled(false);
        jButton_7.setEnabled(false);
        jButton_8.setEnabled(false);
        jButton_9.setEnabled(false);
        jButton_0.setEnabled(false);
        
        jButton_cancelar.setEnabled(false);
        jButton_borrar.setEnabled(false);
        jButton_aceptar.setEnabled(false);
        }
        else if (opcionDepositar == true) {
            opcionDepositar = false;
            
            nuevaCantidad = Double.parseDouble(String.valueOf(txt_cantidad.getText()));
            az.depositar(nuevaCantidad);
            txt_saldo.setText(String.valueOf(az.obtenerSaldo()));
            
            panel_opcion.setVisible(false);
            txt_cantidad.setText(" ");
            jButton_depositar.setEnabled(true);
            jButton_retirar.setEnabled(true);
                    
        jButton_1.setEnabled(false);
        jButton_2.setEnabled(false);
        jButton_3.setEnabled(false);
        jButton_4.setEnabled(false);
        jButton_5.setEnabled(false);
        jButton_6.setEnabled(false);
        jButton_7.setEnabled(false);
        jButton_8.setEnabled(false);
        jButton_9.setEnabled(false);
        jButton_0.setEnabled(false);
        
        jButton_cancelar.setEnabled(false);
        jButton_borrar.setEnabled(false);
        jButton_aceptar.setEnabled(false);             
        }
        else if(opcionRetirar == true){
        opcionRetirar = false;
        
        nuevaCantidad = Double.parseDouble(String.valueOf(txt_cantidad.getText()));
        az.retirar(nuevaCantidad);
        txt_saldo.setText(String.valueOf(az.obtenerSaldo()));
        
    panel_opcion.setVisible(false);
            txt_cantidad.setText(" ");
            jButton_depositar.setEnabled(true);
            jButton_retirar.setEnabled(true);
                    
        jButton_1.setEnabled(false);
        jButton_2.setEnabled(false);
        jButton_3.setEnabled(false);
        jButton_4.setEnabled(false);
        jButton_5.setEnabled(false);
        jButton_6.setEnabled(false);
        jButton_7.setEnabled(false);
        jButton_8.setEnabled(false);
        jButton_9.setEnabled(false);
        jButton_0.setEnabled(false);
        
        jButton_cancelar.setEnabled(false);
        jButton_borrar.setEnabled(false);
        jButton_aceptar.setEnabled(false);    
        }
    }                                               


