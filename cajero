    private void jButton_0ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // boton 0 
        if (txt_cantidad.getText().equals(" ")) {
            txt_cantidad.setText("");
            nuevoNumero = txt_cantidad.getText()+"0";
            txt_cantidad.setText(nuevoNumero);
        }
        else{
        nuevoNumero = txt_cantidad.getText()+"0";
        txt_cantidad.setText(nuevoNumero);
        }
    }                                         

    private void jButton_ingresarActionPerformed(java.awt.event.ActionEvent evt) {                                                 
        // boton ingresar
        txt_nombre.setText(JOptionPane.showInputDialog("Ingrese su Nombre"));
        jButton_ingresar.setEnabled(false);
        jButton_depositar.setEnabled(true);
        jButton_retirar.setEnabled(true);
        jButton_salir.setEnabled(true);
    }                                                

    private void jButton_depositarActionPerformed(java.awt.event.ActionEvent evt) {                                                  
        // Boton depositar
        opcionDepositar = true;
        jButton_retirar.setEnabled(false);
        jButton_depositar.setEnabled(false);
        panel_opcion.setVisible(true);
        
        txt_opcion.setText("[Deposito] -Ingrese cantidad que desea Depositar-");
        
        jButton_1.setEnabled(true);
        jButton_2.setEnabled(true);
        jButton_3.setEnabled(true);
        jButton_4.setEnabled(true);
        jButton_5.setEnabled(true);
        jButton_6.setEnabled(true);
        jButton_7.setEnabled(true);
        jButton_8.setEnabled(true);
        jButton_9.setEnabled(true);
        jButton_0.setEnabled(true);
        
        jButton_cancelar.setEnabled(true);
        jButton_borrar.setEnabled(true);
        jButton_aceptar.setEnabled(true);
    }                                                 

    private void jButton_retirarActionPerformed(java.awt.event.ActionEvent evt) {                                                
        // boton retirar
        opcionRetirar = true;
        jButton_retirar.setEnabled(false);
        jButton_depositar.setEnabled(false);
        panel_opcion.setVisible(true);
        
        txt_opcion.setText("[Retiro] -Ingrese cantidad que desea Retirar-");
        
        jButton_1.setEnabled(true);
        jButton_2.setEnabled(true);
        jButton_3.setEnabled(true);
        jButton_4.setEnabled(true);
        jButton_5.setEnabled(true);
        jButton_6.setEnabled(true);
        jButton_7.setEnabled(true);
        jButton_8.setEnabled(true);
        jButton_9.setEnabled(true);
        jButton_0.setEnabled(true);
        
        jButton_cancelar.setEnabled(true);
        jButton_borrar.setEnabled(true);
        jButton_aceptar.setEnabled(true);
        
    }                                               

    private void jButton_salirActionPerformed(java.awt.event.ActionEvent evt) {                                              
        // boton salir
        if (opcionDepositar == true || opcionRetirar == true) {
         opcionDepositar = false;
         opcionRetirar = false;
         
         panel_opcion.setVisible(false);
         jButton_depositar.setEnabled(true);
         jButton_retirar.setEnabled(true);
         
        jButton_1.setEnabled(false);
        jButton_2.setEnabled(false);
        jButton_3.setEnabled(false);
        jButton_4.setEnabled(false);
        jButton_5.setEnabled(false);
        jButton_6.setEnabled(false);
        jButton_7.setEnabled(false);
        jButton_8.setEnabled(false);
        jButton_9.setEnabled(false);
        jButton_0.setEnabled(false);
        
        jButton_cancelar.setEnabled(false);
        jButton_borrar.setEnabled(false);
        jButton_aceptar.setEnabled(false);
        
        txt_cantidad.setText(" ");
        }
        else{
        JOptionPane.showMessageDialog(null,"Gracias Vuelva Pronto");
        }
    }                                             

